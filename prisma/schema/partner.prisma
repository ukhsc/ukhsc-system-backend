model PartnerSchool {
  id                    Int                  @id @default(autoincrement())
  short_name            String               @unique
  full_name             String               @unique
  plan                  PartnerPlan
  google_account_config SchoolAccountConfig?

  students        StudentMember[]
  personal_orders PersonalMembershipOrder[]
}

enum PartnerPlan {
  Personal
  GroupA // 團體購買（有繳納會費者）
  GroupB // 團體購買（全校學生）
  Combined // 繳納會費（GroupA）＋個人購買方案並行
}
